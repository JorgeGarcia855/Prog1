package co.edu.unbosque.model.persistence;

public class FrasesRegueton {
	
	private String[][] frases;
	
	public FrasesRegueton() {
		frases = new String[4][6];
		matrizFrases();
	}
	
	public void matrizFrases() {
		frases[0][0]= "mami"; 
		frases[0][1]= "bebe";
		frases[0][2]= "princess";
		frases[0][3]= "mami";
		
		frases[1][0]= "yo quiero";
		frases[1][1]= "yo puedo";
		frases[1][2]= "yo vengo a";
		frases[1][3]= "voy a";
		
		frases[2][0]= "encenderte";
		frases[2][1]= "amarte";
		frases[2][2]= "ligar";
		frases[2][3]= "jugar";
		
		frases[3][0]= "suave";
		frases[3][1]= "lento";
		frases[3][2]= "rapido";
		frases[3][3]= "fuerte";
		
		frases[4][0]= "hasta que salga el sol";
		frases[4][1]= "toda la noche";
		frases[4][2]= "hasta el amanecer";
		frases[4][3]= "todo el dia";
		
		frases[5][0]= "sin anestesia";
		frases[5][1]= "sin compromiso";
		frases[5][2]= "feis to feis";
		frases[5][3]= "sin miedo";
	} 
	
	public void generadorCanciones() {
		
	}
}
